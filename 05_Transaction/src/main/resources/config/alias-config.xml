<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC 
	"-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
	
	<!-- typeAliase : DTO 경로를 축약 해 준다. -->
	<!-- alias="사용할 이름" type="클래스 경로" -->
<configuration>
	<typeAliases>
		<typeAlias alias="board" type="com.spring.dto.BoardDTO"/>
	</typeAliases>
</configuration>